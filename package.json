{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "analyze": "ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "pretty-quick --staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ahooks": "^2.10.9",
    "callapp-lib": "^3.5.1",
    "classnames": "^2.3.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-infinite-scroll-component": "^6.1.0",
    "react-vant": "^0.0.26",
    "umi": "^3.5.14",
    "umi-request": "^1.3.9"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.14",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "lint-staged": "^10.0.7",
    "postcss-aspect-ratio-mini": "^1.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
